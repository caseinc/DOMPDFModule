{
    "name": "dino/dompdf-module",
    "type": "library",
    "description": "A Zend Framework 2 module for incorporating DOMPDF support.",
    "keywords": ["pdf","dompdf", "zf2"],
    "homepage": "http://raymondkolbe.com",
    "license": "MIT",
    "authors": [
        {
        "name": "Raymond Kolbe",
        "email": "raymond.kolbe@maine.edu"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "phenx/php-font-lib": "0.2.*",
        "caseinc/dompdf": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "caseinc/dompdf",
                "type": "library",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/caseinc/dompdf",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "psr-0": {
            "DOMPDFModule": "src/"
        }
    }
}
